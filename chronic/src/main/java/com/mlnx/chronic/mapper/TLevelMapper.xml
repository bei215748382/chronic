<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mlnx.chronic.mapper.TLevelMapper" >
  <resultMap id="BaseResultMap" type="com.mlnx.chronic.entity.TLevel" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="name" property="name" jdbcType="VARCHAR" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer" >
    delete from t_level
    where id = #{id}
  </delete>
  <insert id="insert" parameterType="com.mlnx.chronic.entity.TLevel" >
    insert into t_level (id, name)
    values (#{id}, #{name})
  </insert>
  <update id="updateByPrimaryKey" parameterType="com.mlnx.chronic.entity.TLevel" >
    update t_level
    set name = #{name}
    where id = #{id}
  </update>
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
    select id, name
    from t_level
    where id = #{id}
  </select>
  <select id="selectAll" resultMap="BaseResultMap" >
    select id, name
    from t_level
  </select>
</mapper>